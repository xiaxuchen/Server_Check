<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxyz.check.dao.SchoolDao">

    <select id="getCurrentTermId" resultType="Integer">
        SELECT
        `term.id`
        FROM tb_school
        WHERE id=#{schoolId};
    </select>

</mapper>