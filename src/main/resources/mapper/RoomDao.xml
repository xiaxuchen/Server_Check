<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxyz.check.dao.RoomDao">

    <select id="getRoomName" resultType="String">
        SELECT
        name
        FROM tb_room
        WHERE id=#{id};
    </select>
</mapper>
