<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxyz.check.dao.TimesDao">

    <select id="getTermTimes" resultType="Times">
        SELECT
        id,
        `session`,
        start,
        `end`
        FROM tb_times
        WHERE `term.id`=#{termId};
    </select>

    <select id="getSession" resultType="Integer">
        SELECT
        session
        FROM tb_times
        WHERE id=#{id}
    </select>

</mapper>